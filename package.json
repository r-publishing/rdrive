{
  "name": "rpub-exe",
  "version": "0.0.1",
  "description": "RDrive, an RChain powered remote file system with fuse",
  "private": true,
  "main": "index.js",
  "type": "module",
  "scripts": {
    "start": "nodemon -e ts --exec \"npm run watch\"",
    "watch": "tsc  -b --verbose && node --experimental-specifier-resolution=node ./dist/index.js --run --boxName theo --masterRegUri qcbdxbwmk89kpmsa9iy6hnfj89pgm7y1q9ufonnut5ad66m9ae381q --privKey 6428f75c09db8b3a260fc1dcb1c93619bd3eecf6787b003ddc6ba5e87025c177 --readOnlyHost http://127.0.0.1:40403 --mnt /home/theoxd/demo/rdrive",
    "compile": "tsc  -b --verbose",
    "build": "rm -rf pkg && pika build . && cd pkg && pkg . --out-path dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/r-publishing/rchain-fuse-js.git"
  },
  "author": "Theo Hallenius, RChain Publishing LCA",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/r-publishing/rchain-fuse-js/issues"
  },
  "homepage": "https://github.com/r-publishing/rchain-fuse-js#readme",
  "bin": "dist-node/index.js",
  "dependencies": {
    "@vercel/ncc": "^0.32.0",
    "bindings": "^1.5.0",
    "chalk": "^3.0.0",
    "command-line-args": "^5.2.1",
    "did-resolver": "^2.2.0",
    "dids": "*",
    "fuse-native": "^2.2.6",
    "fuse-shared-library": "^1.0.2",
    "key-did-resolver": "^0.2.4",
    "lodash": "^4.17.21",
    "lodash.clonedeep": "^4.5.0",
    "nanoresource": "^1.3.0",
    "node-fuse-bindings": "^2.12.4",
    "os-service": "^2.2.0",
    "queued-jobs": "^1.1.0",
    "rchain-did-resolver": "*",
    "rchain-token": "*",
    "rchain-toolkit": "^0.3.5",
    "workerpool": "^6.2.0"
  },
  "engines": {
    "node": "16.x"
  },
  "devDependencies": {
    "@pika/pack": "^0.5.0",
    "@pika/plugin-build-node": "^0.8.1",
    "@pika/plugin-copy-assets": "^0.8.1",
    "@pika/plugin-standard-pkg": "^0.8.1",
    "@pika/plugin-ts-standard-pkg": "^0.9.2",
    "nodemon": "^2.0.15",
    "pika-plugin-pkg-node": "*",
    "typescript": "^4.5.2"
  },
  "@pika/pack": {
    "pipeline": [
      [
        "@pika/plugin-standard-pkg"
      ],
      [
        "@pika/plugin-ts-standard-pkg",
        {
          "tsconfig": "tsconfig.pika.json"
        }
      ],
      [
        "@pika/plugin-build-node"
      ],
      [
        "@pika/plugin-copy-assets",
        {
          "assets": [
            "assets/**/*",
            "node_modules/fuse-native/build/Release/*"
          ]
        }
      ]
    ]
  }
}
